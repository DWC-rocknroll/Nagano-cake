<div class="container">
    <div class="row">
      <div class="col-xs-12">
        <h2 class="head-line text-center title_h">配送先編集</h2>
        <%= form_with(model: @deliveries, url:public_delivery_path, local: true, method: :patch) do |f| %>
          <div class="deliveries col-xs-10">
            <div class="form-group col-xs-12">
              <%= f.label:"郵便番号（ハイフンなし）",class:"col-xs-3 control-label" %>
              <%= f.text_field :postcode, class:"col-xs-2 form-control-static postal_code " %>
            </div>
            <div class="form-group col-xs-12">
              <%= f.label:"住所",class:"col-xs-3 control-label" %>
              <%= f.text_field :adress, class:"col-xs-9 form-control-static address " %>
            </div>
            <div class="form-group col-xs-12 ">
              <%= f.label:"宛名",class:"col-xs-3 control-label" %>
              <%= f.text_field :name, class:"col-xs-4 form-control-static name" %>
              <%= f.submit:"変更内容を保存",class:"btn btn-success" %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
</div>
