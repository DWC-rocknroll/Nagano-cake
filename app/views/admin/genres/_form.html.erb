<%= form_with model:@genre, url:admin_genres_path local:true do |f| %>
  <form class="form-inline">
    <div class="form-group">
      <%= f.label :name, "ジャンル名" %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.submit class: 'bnt btn-success' %>
    </div>
  </form>
<% end %>